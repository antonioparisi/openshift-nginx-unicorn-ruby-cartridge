#!/bin/bash -e

case "$1" in
  -v|--version)
    version="$2"
esac

# Parse arguments
source "lib/util"
parse_args $@

# Don't set -u until after the above arg parsing is complete
set -u

echo "$version" > env/NGINX_VERSION


for dir in logs run; do
  mkdir -p $dir
done

# rbenv
#source "lib/rbenv"
